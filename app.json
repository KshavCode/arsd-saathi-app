{
  "expo": {
    "name": "ArsdSaathi",
    "slug": "arsdsaathi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.webp",
    "scheme": "arsdsaathi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.keshav.arsdsaathi"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon.webp"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.keshav.arsdsaathi",
      "versionCode": 1
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.webp"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.webp",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "46d75500-c0b1-4209-be34-625a90ced4d1"
      }
    }
  }
}
